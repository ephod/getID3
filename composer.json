{
  "name": "ephod/getid3",
  "authors": [
    {
      "name": "Michael A. Johnson Lucas",
      "email": "michael.johnson.lucas@gmail.com",
      "role": "developer"
    },
    {
      "name": "James Heinrich",
      "email": "info@silisoftware.com",
      "role": "developer",
      "homepage": "http://www.silisoftware.com/"
    }
  ],
  "description": "PHP script that extracts useful information from popular multimedia file formats",
  "homepage": "http://www.getid3.org/",
  "keywords": [
    "php",
    "tags",
    "codecs"
  ],
  "type": "library",
  "license": [
    "GPL-1.0-or-later",
    "LGPL-3.0-only",
    "MPL-2.0"
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "0.9.2",
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "lint": "parallel-lint --exclude vendor .",
    "test": [
      "composer lint"
    ]
  },
  "autoload": {
    "classmap": [
      "getid3/"
    ],
    "psr-4": {
      "GetId3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GetId3\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1.0"
    }
  }
}
